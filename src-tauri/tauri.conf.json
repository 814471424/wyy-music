{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:3005",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "music-app",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "startDragging": true,
        "create": true,
        "setFocus": true,
        "maximize": true,
        "unmaximize": true,
        "minimize": true,
        "close": true,
        "hide": true
      },
      "fs": {
        "readFile": false,
        "readDir": true,
        "scope": [
          "**"
        ]
      },
      "dialog": {
        "open": true,
        "save": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.dev",
      "targets": "all",
      "windows": {
        "certificateThumbprint": "9FC7EDCA0EB25DC84A7230825CCB03D3184FC909",
        "digestAlgorithm": "sha256",
        "timestampUrl": "http://timestamp.comodoca.com"
      }
    },
    "security": {
      "csp": "stream: https://stream.localhost asset: https://asset.localhost"
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://gitee.com/qq814471424/wyy-music/raw/master/update.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDkxQ0U3NzFBMEREMTg0MUIKUldRYmhORU5HbmZPa1hxTjdjK0UxWWJSRkZOMnF5MllIWWFGRTFESmFrZ1JkTlhJZ3JSdDFBc08K"
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "music-app",
        "width": 1020,
        "height": 670,
        "minWidth": 1020,
        "minHeight": 670,
        "decorations": false,
        "fileDropEnabled": false
      }
    ]
  }
}